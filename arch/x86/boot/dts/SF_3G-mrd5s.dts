/ {
	#address-cells = <1>;
	#size-cells = <1>;
	model = "intel,sofia_3g_soc-es2";
	compatible = "intel,sofia_3g_soc-svb", "intel,xgold";

	#include "xgold/sofia_3g_soc/sofia_3g_soc.dtsi"
