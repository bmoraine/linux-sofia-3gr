		pm,user-name = "bl";
		pm,state-D0 = "enable";
		pm,state-D3 = "disable";
		intel,led-k2 = <0x1B0>;
		intel,led-k1max = <0x122>;
		intel,led-k2max = <0xFFFF>;
		intel,led-up = <0x104>;
		intel,led-down = <0x200>;
		intel,led-safe = <0x12>;
		pinctrl-2 = <&led_agold_inactive_pins>;
        };
#endif

