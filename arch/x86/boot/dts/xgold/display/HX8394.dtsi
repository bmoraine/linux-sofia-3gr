	hx8394 {
		compatible = "intel,display";
		intel,display-resolution= <720 1280>;
		intel,display-dpi= <240 240>;

		/* MIPI-DSI video mode settings */
		intel,display-vid-hfp-hs-bytes = <90>;
		intel,display-vid-hbp-hs-bytes = <90>;
		intel,display-vid-hsa-hs-bytes = <12>;
		intel,display-vid-vfp-lines = <8>;
		intel,display-vid-vbp-lines = <8>;
		intel,display-vid-vsa-lines = <4>;

		intel,display-vid-mode = "pulses";
		intel,display-vid-id = <0>;
		intel,display-vid-pixel = "24packed";
		intel,display-vid-fill_buf_to = <0x1F>;

		intel,display-reset = <1 1 0 2 1 10>;

		cmd-init {
 			access-ext-commands {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB9 0xFF 0x83 0x94>;
				intel,cmd-lp = <1>;
			};
 			set-mipi-control {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xBA 0x33 0x83>;
				intel,cmd-lp = <1>;
			};
 			set-power {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB1
					0x6C 0x15 0x15 0x24 0xE4 0x11 0xF1 0x80 0xE4 0x97
					0x23 0x80 0xC0 0xD2 0x58>;
				intel,cmd-lp = <1>;
			};
 			set-display {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB2
					0x00 0x64 0x10 0x07 0x22 0x1C 0x08 0x08 0x1C 0x4D 0x00>;
				intel,cmd-lp = <1>;
			};
 			set-dring-timing {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB4
					0x00 0xFF 0x03 0x5A 0x03 0x5A 0x03 0x5A 0x01 0x6A 0x30 0x6A>;
				intel,cmd-lp = <1>;
			};
 			set-cmd-0 {
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0xBC 0x07>;
				intel,cmd-lp = <1>;
			};
 			set-cmd-1 {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xBF 0x41 0x0E 0x01>;
				intel,cmd-lp = <1>;
			};
 			set-cmd-2 {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xD3
					0x00 0x06 0x00 0x40 0x07 0x08 0x00 0x32 0x10 0x07
					0x00 0x07 0x54 0x15 0x0F 0x05 0x04 0x02 0x12 0x10
					0x05 0x07 0x33 0x33 0x0B 0x0B 0x37 0x10 0x07 0x07>;
				intel,cmd-lp = <1>;
			};
 			set-gip {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xD5
					0x04 0x05 0x06 0x07 0x00 0x01 0x02 0x03 0x20 0x21
					0x22 0x23 0x18 0x18 0x18 0x18 0x18 0x18 0x18 0x18
					0x19 0x19 0x18 0x18 0x18 0x18 0x1B 0x1B 0x1A 0x1A
					0x18 0x18 0x18 0x18 0x18 0x18 0x18 0x18 0x18 0x18
					0x18 0x18 0x18 0x18>;
				intel,cmd-lp = <1>;
			};
 			set-cmd-3 {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xD6
					0x03 0x02 0x01 0x00 0x07 0x06 0x05 0x04 0x23 0x22
					0x21 0x20 0x18 0x18 0x18 0x18 0x18 0x18 0x58 0x58
					0x18 0x18 0x19 0x19 0x18 0x18 0x1B 0x1B 0x1A 0x1A
					0x18 0x18 0x18 0x18 0x18 0x18 0x18 0x18 0x18 0x18
					0x18 0x18 0x18 0x18>;
				intel,cmd-lp = <1>;
			};
 			set-panel {
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0xCC 0x01>;
				intel,cmd-lp = <1>;
			};
 			set-vcom-volt {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB6 0x68 0x68>;
				intel,cmd-lp = <1>;
			};
 			set-gamma {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xE0
					0x00 0x10 0x16 0x2D 0x33 0x3F 0x23 0x3E 0x07 0x0B
					0x0D 0x17 0x0E 0x12 0x14 0x12 0x13 0x06 0x11 0x13
					0x18 0x00 0x0F 0x16 0x2E 0x33 0x3F 0x23 0x3D 0x07
					0x0B 0x0D 0x18 0x0F 0x12 0x14 0x12 0x14 0x07 0x11
					0x12 0x17>;
				intel,cmd-lp = <1>;
			};
 			set-cmd-4 {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xC0 0x30 0x14>;
				intel,cmd-lp = <1>;
			};
 			set-cmd-5 {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xC7 0x00 0xC0 0x40 0xC0>;
				intel,cmd-lp = <1>;
			};
 			set-cmd-6 {
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0xDF 0x87>;
				intel,cmd-lp = <1>;
			};
 			set-cmd-7 {
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0xD2 0x66>;
				intel,cmd-lp = <1>;
			};
 			set-addr-mod {
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0x36 0x0A>;
				intel,cmd-lp = <1>;
			};
			cabc-st-mode{
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0x55 0x02>;
				intel,cmd-lp = <1>;
			};
			max-brightness{
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0x51 0xF0>;
				intel,cmd-lp = <1>;
			};
			min-brightness{
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0x5E 0x00>;
				intel,cmd-lp = <1>;
			};
			cabc-enable{
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0x53 0x24>;
				intel,cmd-lp = <1>;
			};
			sleep-out {
				intel,cmd-type = <0x05>;
				intel,cmd-data = <0x11>;
				intel,cmd-delay = <200>;
				intel,cmd-lp = <1>;
			};
			display-on {
				intel,cmd-type = <0x05>;
				intel,cmd-data = <0x29>;
				intel,cmd-lp = <1>;
			};
		};

		cmd-power-on {
			display-on {
				intel,cmd-type = <0x05>;
				intel,cmd-data = <0x29>;
				intel,cmd-lp = <1>;
			};
		};

		cmd-power-off {
			display-off {
				intel,cmd-type = <0x05>;
				intel,cmd-data = <0x28>;
				intel,cmd-lp = <1>;
			};
		};

		cmd-sleep-in {
			display-off {
				intel,cmd-type = <0x05>;
				intel,cmd-data = <0x28>;
				intel,cmd-lp = <1>;
			};
			sleep-in {
				intel,cmd-type = <0x05>;
				intel,cmd-data = <0x10>;
				intel,cmd-delay = <200>;
				intel,cmd-lp = <1>;
			};
		};

		cmd-sleep-out {
			sleep-out {
				intel,cmd-type = <0x05>;
				intel,cmd-data = <0x11>;
				intel,cmd-delay = <200>;
				intel,cmd-lp = <1>;
			};
			display-on {
				intel,cmd-type = <0x05>;
				intel,cmd-data = <0x29>;
				intel,cmd-lp = <1>;
			};
		};

	};
