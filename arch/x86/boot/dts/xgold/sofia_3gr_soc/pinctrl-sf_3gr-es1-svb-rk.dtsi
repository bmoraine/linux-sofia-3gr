	pinctrl-single,pins = <
		0x3a0 0x1410	/* sys_clk_en */
	>;
};
sys_inactive_pins:sys_inactive_pins {
	pinctrl-single,pins = <
		0x3a0 0x1410	/* sys_clk_en */
	>;
};
sys_sleep_pins:sys_sleep_pins {
