&xgold_pmx_gpio {
		intel,gpio-to-irq-num = < 9 >;
		intel,gpio-to-irq = <
			53 EXI0
			1  EXI2
			9  EXI3
			2  EXI4
			0  EXI5
			5  EXI7
			32 EXI11
			18 EXI12
			34 EXI14 >;
};
&xgold_pmx_core {
	/* sparse GPIO range could be supported */
		/*first pin / nb pins / value */
	pinctrl-single,gpio-range =
		<&range 0 19 0x100>, 
		<&range 19 1 0x1700>,/*gpio19, touch reset, output, hight, PP*/
		<&range 20 16 0x100>,
		<&range 36 1 0x1700>,/*gpio36, LCD reset, output, hight, PP*/
		<&range 37 18 0x100>,
		<&range 55 1 0x1700>,/*gpio55, HDMI reset, output, hight, PP*/
		<&range 56 49 0x100>;

	pinctrl-names = "default", "sleep", "deactivate"; /*"activate", "sleep", "deactivate" */
	/* map here static pins (do not need any dynamic config) */
	pinctrl-0 = <>;
	pinctrl-1 = <>;
	pinctrl-2 = <>;
